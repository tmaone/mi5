GEMINI_VENDOR="${ANDROID_ROOT}/vendor/xiaomi/gemini"
VENDOR_MK="${GEMINI_VENDOR}/gemini-vendor.mk"
ANDROID_MK="${GEMINI_VENDOR}/Android.mk"


append()
{
	echo "${@}"
	echo "${@}" >> "${ANDROID_MK}"
}

vappend()
{
	echo "${@}"
	echo "${@}" >> "${VENDOR_MK}"
}


vappend ''
