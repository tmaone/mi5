KEYBACK_MOD_PATH="${MODS_DIR}/keyback"

KEYLAYOUT="${KEYBACK_MOD_PATH}/synaptics_dsx.kl"

GEMINI_DEVICE="${ANDROID_ROOT}/device/xiaomi/gemini"

KEYLAYOUT_TARGET="${GEMINI_DEVICE}/keylayout/synaptics_dsx.kl"


keyback_enable()
{
	cp -v "${KEYLAYOUT}" "${KEYLAYOUT_TARGET}"
}

keyback_disable()
{
	return 0
}


