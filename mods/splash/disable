MOD_ORIGINAL="${ANDROID_ROOT}/vendor/xiaomi/gemini/radio/splash.img"

MOD_FILE="${MODS_DIR}/splash/splash.img"

MOD_REFRESH="${OUT_DIR}/target/product/gemini/splash.img"

file_ok "${MOD_FILE}.bak" && out "Restore backup [${MOD_FILE}.bak] to [${MOD_ORIGINAL}]" && mv "${MOD_FILE}.bak" "${MOD_ORIGINAL}"
file_ok "${MOD_REFRESH}" && out "Trigerring refresh [${MOD_REFRESH}]" && rm "${MOD_REFRESH}"
