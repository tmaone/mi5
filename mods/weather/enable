MOD_DIR="${MODS_DIR}/weather"
MOD_MK="${MOD_DIR}/YahooAndroid.mk"
MOD_DEST="${ANDROID_ROOT}/packages/apps/YahooWeatherProvider"
GEMINI_VENDOR="${ANDROID_ROOT}/vendor/xiaomi/gemini/gemini-vendor.mk"

out "Install native Android.mk [${MOD_MK}] to [${MOD_DEST}/Android.mk]"
cp -v "${MOD_MK}" "${MOD_DEST}/Android.mk"

vappend()
{
	echo "${@}"
	echo "${@}" >> "${GEMINI_VENDOR}"
}

vappend ''
vappend 'PRODUCT_PACKAGES += YahooWeatherProvider'
vappend ''
