# // file_ok "${MOD_DIR}/gemini-vendor.mk" && mv -v "${MOD_DIR}/gemini-vendor.mk" "${VENDOR_MK}"
# //
# // out "Restore android  makefile  [${ANDROID_MK}] from [${MOD_DIR}/Android.mk]"
# // file_ok "${MOD_DIR}/Android.mk" && mv -v "${MOD_DIR}/Android.mk" "${ANDROID_MK}"
